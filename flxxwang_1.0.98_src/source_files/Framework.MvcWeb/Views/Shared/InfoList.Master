                      { %>
                    <tr id="trPersonal"><td class="dev" colspan="4"><%=ViewTag.Interface.City.ShortName %><%=curInfoType %>- 个人信息 [<a href="<%=Url.AddRouteValues(routeData,new{biz=0,pageindex=1}) %>" title="只看个人信息">只看个人</a>]</td></tr>
                    <%} %>

                    <tr>

                %>
                <dl class="secitem">
                    <dt>类型：</dt>
                    <dd>
                        <%
                            if (ViewTag.Tag == null)
                        {
                            %>
                                <a href="<%=Url.RemoveRouteValues(routeData,new string[]{"tag","pageindex"})%>" class='select'>全部</a>
                            <%
                        }
